{
  "name": "bren-icons",
  "version": "1.1.0",
  "description": "Uma biblioteca completa de 116+ ícones SVG personalizados para React, construída com TypeScript e otimizada para performance",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:vite": "vite build",
    "dev": "rollup -c -w",
    "dev:vite": "vite",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "rename-svg": "node scripts/rename-svg-files.js",
    "fix-names": "node scripts/fix-remaining-names.js",
    "fix-svg": "node scripts/fix-svg-and-names.js",
    "generate-icons": "node scripts/generate-icons.js",
    "publish:check": "node scripts/publish.js"
  },
  "keywords": [
    "icons",
    "svg",
    "react",
    "typescript",
    "ui",
    "design-system"
  ],
  "author": "Bren",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/react": "^18.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "rimraf": "^5.0.0",
    "rollup": "^3.0.0",
    "rollup-plugin-dts": "^5.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "typescript": "^5.0.0",
    "vite": "^4.0.0",
    "vite-plugin-dts": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SrPalha/Bren-Icons.git"
  },
  "homepage": "https://github.com/SrPalha/Bren-Icons#readme",
  "bugs": {
    "url": "https://github.com/SrPalha/Bren-Icons/issues"
  },
  "dependencies": {
    "tslib": "^2.8.1"
  }
}
